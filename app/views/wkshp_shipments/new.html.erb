<% provide(:page_name, "Shipment") %>
<table>
<%= form_for(@shipment) do |f| %>

<tr><td>Description: <td><%= f.text_field :description %></td></tr>
<tr><td>Date Captured: <td><%= f.text_field :departure_date %></td></tr>
<tr><td>Expected Delivery Date: <td><%= f.text_field :delivery_date %></td></tr>
<tr><td></td><td align="right"><%= f.submit("Save", :class=>"btn btn-medium btn-secondary")%></td></tr>
<% end %>
</table>